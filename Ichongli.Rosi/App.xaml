<Application
    x:Class="Ichongli.Rosi.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:phone="clr-namespace:Microsoft.Phone.Controls;assembly=Microsoft.Phone"
    xmlns:shell="clr-namespace:Microsoft.Phone.Shell;assembly=Microsoft.Phone"
    xmlns:local="clr-namespace:Ichongli.Rosi">

    <!--应用程序资源-->
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Assets/GlobalResources.xaml" />
                <ResourceDictionary Source="Assets/LocalResources.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <local:AppBootstrapper x:Key="AppBootstrapper"/>
        </ResourceDictionary>
    </Application.Resources>

</Application>